<configuration>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Frame-Options" />
        <add name="Content-Security-Policy"
             value="frame-ancestors https://teams.microsoft.com https://*.teams.microsoft.com;" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>